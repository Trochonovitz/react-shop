{
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": 0,
    "prettier/prettier": ["error", { "singleQuote": true, "endOfLine": "auto" }]
  }
}
